{
  "button": {
    "colours": {
      "normal_bg": "#45494e",
      "hovered_bg": "#35393e",
      "disabled_bg": "#25292e",
      "selected_bg": "#193754",
      "active_bg": "#193754",
      "dark_bg": "#15191e",
      "normal_text": "#c5cbd8",
      "hovered_text": "#FFFFFF",
      "selected_text": "#FFFFFF",
      "disabled_text": "#6d736f",
      "active_text": "#FFFFFF",
      "normal_border": "#DDDDDD",
      "hovered_border": "#B0B0B0",
      "disabled_border": "#808080",
      "selected_border": "#8080B0",
      "active_border": "#8080B0"
    },
    "font": {
      "name": "HanZi",
      "size": "40",
      "regular_resource": {
        "package": "resources.fonts",
        "resource": "HanZi.ttf"
      }
    },
    "misc": {
      "border_width": "0.5",
      "tool_tip_delay": "1.0"
    }
  },
  "window": {
    "colours": {
      "dark_bg": "#21282D",
      "normal_border": "#999999"
    },
    "misc": {
      "shape": "rounded_rectangle",
      "shape_corner_radius": "10",
      "border_width": "1",
      "shadow_width": "15",
      "title_bar_height": "25"
    }
  },
  "window.#title_bar": {
    "font": {
      "name": "HanZi1",
      "size": "12",
      "regular_resource": {
        "package": "resources.fonts",
        "resource": "HanZi.ttf"
      }
    }
  },
  "window.#close_button": {
    "font": {
      "name": "HanZi2",
      "size": "14",
      "regular_resource": {
        "package": "resources.fonts",
        "resource": "HanZi.ttf"
      }
    },
    "misc": {
      "text_vert_alignment": "center",
      "text_horiz_alignment": "center",
      "tool_tip_delay": "1.0"
    }
  },
  "#confirmation_dialog.#confirm_button": {
    "font": {
      "name": "HanZi",
      "size": "14",
      "regular_resource": {
        "package": "resources.fonts",
        "resource": "HanZi.ttf"
      }
    },
    "misc": {
      "shape": "rounded_rectangle",
      "border_width": "0.5",
      "tool_tip_delay": "1.0"
    }
  },
  "#confirmation_dialog.#cancel_button": {
    "font": {
      "name": "HanZi",
      "size": "14",
      "regular_resource": {
        "package": "resources.fonts",
        "resource": "HanZi.ttf"
      }
    },
    "misc": {
      "shape": "rounded_rectangle",
      "border_width": "0.5",
      "tool_tip_delay": "1.0"
    }
  },
  "#message_window.#dismiss_button": {
    "font": {
      "name": "HanZi",
      "size": "16",
      "regular_resource": {
        "package": "resources.fonts",
        "resource": "HanZi.ttf"
      }
    }
  },
  "selection_list": {
    "colours": {
      "dark_bg": "#21282D",
      "normal_border": "#999999"
    }
  },
  "selection_list.@selection_list_item": {
    "font": {
      "name": "HanZi",
      "size": "8",
      "regular_resource": {
        "package": "resources.fonts",
        "resource": "HanZi.ttf"
      }
    }
  },
  "screen_space_health_bar": {
    "colours": {
      "normal_text": "#c5cbd8",
      "text_shadow": "#777777",
      "normal_border": "#DDDDDD",
      "filled_bar": "#f4251b,#A4150b,180",
      "unfilled_bar": "#CCCCCC"
    },
    "font": {
      "name": "Hanzi",
      "size": "14",
      "regular_resource": {
        "package": "resources.fonts",
        "resource": "HanZi.ttf"
      }
    }
  },
  "text_box": {
    "colours": {
      "dark_bg": "#21282D",
      "normal_border": "#999999",
      "link_text": "#FF0000",
      "link_hover": "#FFFF00",
      "link_selected": "#FFFFFF"
    },
    "misc": {
      "border_width": "1",
      "padding": "10,10",
      "link_normal_underline": "0",
      "link_hover_underline": "1"
    }
  },
  "text_box.vertical_scroll_bar": {
    "colours": {
      "dark_bg": "#505068"
    }
  },
  "text_box.vertical_scroll_bar.#sliding_button": {
    "misc": {
      "border_width": "1"
    }
  }
}